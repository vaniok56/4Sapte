[default]
; Configuration template for Second-Hand Market Bot
; Copy this file to config.ini and fill in your actual values

; Telegram Bot Configuration
; Get these from https://my.telegram.org/auth and @BotFather
BOT_TOKEN = your_telegram_bot_token_here
api_id = your_telegram_api_id_here
api_hash = your_telegram_api_hash_here

; AI Service / OpenRouter API Configuration
; Provide your AI service API key and endpoint
AI_API_KEY = your_openrouter_api_key_here
AI_API_URL = "https://openrouter.ai/api/v1/chat/completions"
AI_MODEL = "meta-llama/llama-4-maverick:free"